{
  "name": "skyhigh-core-checkin-system",
  "version": "1.0.0",
  "description": "SkyHigh Core Digital Check-In System - High-performance microservices backend",
  "main": "dist/index.js",
  "scripts": {
    "dev:gateway": "ts-node src/api-gateway/index.ts",
    "dev:seat": "ts-node src/seat-service/index.ts",
    "dev:checkin": "ts-node src/checkin-service/index.ts",
    "dev:payment": "ts-node src/payment-service/index.ts",
    "dev:waitlist": "ts-node src/waitlist-service/index.ts",
    "dev:notification": "ts-node src/notification-service/index.ts",
    "dev:weight": "ts-node src/weight-service/index.ts",
    "dev:abuse": "ts-node src/abuse-detection-service/index.ts",
    "watch:seat": "nodemon --legacy-watch src/seat-service/index.ts",
    "watch:checkin": "nodemon --legacy-watch src/checkin-service/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=tests/unit",
    "test:integration": "jest --testPathPattern=tests/integration",
    "test:e2e": "jest --testPathPattern=tests/e2e",
    "test:coverage": "jest --coverage",
    "seed": "ts-node scripts/seed-data.ts",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\""
  },
  "keywords": [
    "microservices",
    "airline",
    "checkin",
    "seat-management",
    "typescript",
    "mongodb",
    "redis"
  ],
  "author": "SkyHigh Core Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.5",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "prom-client": "^15.1.0",
    "qrcode": "^1.5.3",
    "rate-limit-redis": "^4.2.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "prettier": "^3.1.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
